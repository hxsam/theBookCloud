<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
 <class name="com.thebookcloud.objects.AuthorRating" table="Author_Rating" >
  <composite-id>
    <key-property name="authorId" column="author_id" type="int" /> 
    <key-property name="userId" column="user_id" type="int" /> 
  </composite-id>
 
  <property name="rating">
   <column name="rating"/>
  </property>
  <property name="review">
   <column name="review"/>
  </property>
 </class>
</hibernate-mapping>
