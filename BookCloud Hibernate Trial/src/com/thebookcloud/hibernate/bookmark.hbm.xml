<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
 <class name="com.thebookcloud.objects.Bookmark" table="Bookmark">
  <id name="bookmarkId" type="int" column="bookmark_id" >
   <generator class="native"/>
  </id>
 
  <property name="userId">
   <column name="user_id" />
  </property>  
  <property name="bookId">
   <column name="book_id" />
  </property>
  <property name="chapterId">
   <column name="chapter_id"/>
  </property>
  <property name="firstLineOfPage">
   <column name="first_line_of_page"/>
  </property>
 </class>
</hibernate-mapping>

